{
  "name": "operations-data-manager",
  "version": "1.0.0",
  "description": "Operations department data management application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "install-deps": "cd backend && npm install && cd ../frontend && npm install",
    "start-backend": "cd backend && node server.js",
    "start-frontend": "cd frontend && npm start"
  },
  "keywords": [
    "operations",
    "data-management",
    "excel",
    "real-estate"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jspdf": "^1.3.3",
    "concurrently": "^8.2.2",
    "jspdf": "^3.0.1"
  },
  "dependencies": {
    "html2pdf.js": "^0.10.3"
  }
}
